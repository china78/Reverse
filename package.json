{
  "name": "chatgpt-t2g",
  "private": false,
  "license": "mit",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "next dev",
    "build": "cross-env BUILD_MODE=standalone next build",
    "start": "next start",
    "lint": "next lint",
    "export": "cross-env BUILD_MODE=export BUILD_APP=1 next build",
    "export:dev": "cross-env BUILD_MODE=export BUILD_APP=1 next dev",
    "app:dev": "yarn tauri dev",
    "app:build": "yarn tauri build",
    "prompts": "node ./scripts/fetch-prompts.mjs",
    "prepare": "husky install",
    "proxy-dev": "sh ./scripts/init-proxy.sh && proxychains4 -f ./scripts/proxychains.conf yarn dev",
    "proxy-dev-direct": "proxychains4 -f ./scripts/proxychains.conf yarn dev",
    "init-dev-db": "prisma migrate dev --name init",
    "init-prod-db": "prisma migrate deploy"
  },
  "dependencies": {
